    <webLinks>
        <fullName>Lottery_Random</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Run Lottery</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 

var lotteryStatus = &apos;{!Listing__c.Lottery_Status__c}&apos;;
if( lotteryStatus== &quot;Lottery Complete&quot; || lotteryStatus == &quot;In Progress&quot;){
 alert(&quot;Lottery has already been run&quot;);
} else {

message = sforce.apex.execute(&quot;LOT_RandomDrawService&quot;, &quot;runLottery&quot;, {listingID:&quot;{!Listing__c.Id}&quot;}); 
if (message != &apos;&apos;) 
{ 
alert(message); 
} else {
//alert(&apos;ok&apos;);
navigateToUrl(&apos;/{!Listing__c.Id}&apos;);
}
}</url>
