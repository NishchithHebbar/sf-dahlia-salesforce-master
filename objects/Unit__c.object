        <fullName>Min_Occupancy__c</fullName>
        <description>Field will ultimately be hidden from page layout and is automatically determined based on number of Bedrooms. Field is used to hold the minimum occupancy based on bedrooms.</description>
        <externalId>false</externalId>
        <formula>/*OLD FORMULA - Replaced 10/13. Pivotal story: https://www.pivotaltracker.com/story/show/131777421
/*IF (Number_of_Bedrooms__c &gt;1, Number_of_Bedrooms__c, 1)*/

IF(ISPICKVAL(Unit_Type__c,&apos;2 BR&apos;), 2, 
  IF(ISPICKVAL(Unit_Type__c,&apos;3 BR&apos;), 3,
    IF(ISPICKVAL(Unit_Type__c,&apos;4 BR&apos;), 4,
      IF(ISPICKVAL(Unit_Type__c,&apos;5 BR&apos;),5,
     1))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Field is used to hold the minimum occupancy based on bedrooms.</inlineHelpText>
        <label>Min Occupancy</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
