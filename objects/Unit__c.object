    <fields>
        <fullName>Number_of_bedrooms_formula__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Unit_Type__c,&apos;Large Studio&apos;), 1, 
IF(ISPICKVAL(Unit_Type__c,&apos;1 BR&apos;), 1,
IF(ISPICKVAL(Unit_Type__c,&apos;2 BR&apos;), 2, 
IF(ISPICKVAL(Unit_Type__c,&apos;3 BR&apos;), 3,
IF(ISPICKVAL(Unit_Type__c,&apos;4 BR&apos;), 4,
IF(ISPICKVAL(Unit_Type__c,&apos;5 BR&apos;),5,
0))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Number of bedrooms</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
