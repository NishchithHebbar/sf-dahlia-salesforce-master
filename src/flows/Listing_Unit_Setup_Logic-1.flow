    <decisions>
        <name>Does_Listing_Unit_Exist</name>
        <label>Does Listing Unit Exist?</label>
        <locationX>407</locationX>
        <locationY>331</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Yes_Listing_Unit_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ExistingListingUnit</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Existing_Listing_Unit</targetReference>
            </connector>
            <label>Yes - Listing Unit Exists</label>
        </rules>
        <rules>
            <name>No_Listing_Unit</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ExistingListingUnit</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_new_Listing_Unit</targetReference>
            </connector>
            <label>No Listing Unit</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Listing_Active</name>
        <label>Is Listing Active (Not complete)?</label>
        <locationX>417</locationX>
        <locationY>95</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Yes_Active</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Listing.Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Complete</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Listing_Unit</targetReference>
            </connector>
            <label>Yes - Active</label>
        </rules>
    </decisions>
