    <fields>
        <fullName>Receives_Preference__c</fullName>
        <externalId>false</externalId>
        <formula>OR(

And(Auto_Grant_Count__c &gt; 0,NOT(ISPICKVAL(MOHCD_Preference_Assertion__c, &apos;Invalid for lottery&apos;))),
AND(NOT(ISBLANK(Application_Member__c)), NOT(ISPICKVAL(Lottery_Status__c, &apos;Invalid for lottery&apos;)), NOT(ISPICKVAL(MOHCD_Preference_Assertion__c, &apos;Invalid for lottery&apos;))), 
ISPICKVAL(Lottery_Status__c, &apos;Valid - Added&apos;),
ISPICKVAL(MOHCD_Preference_Assertion__c, &apos;Valid for lottery&apos;)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Receives Preference</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
