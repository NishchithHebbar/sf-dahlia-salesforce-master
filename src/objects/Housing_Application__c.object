    <validationRules>
        <fullName>Disqualification_requires_Disq_Reason</fullName>
        <active>true</active>
        <description>If Household Status is &quot;Disqualified&quot;, Reason for Disqualification must be filled out.</description>
        <errorConditionFormula>AND(ISPICKVAL(Household_Status__c,&quot;Disqualified&quot;),  Reason_for_Disqualification__c = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Reason_for_Disqualification__c</errorDisplayField>
        <errorMessage>If Household Status is &quot;Disqualified&quot;, Reason for Disqualification must be filled out.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lottery_Numbers_Entered_Do_Not_Match</fullName>
        <active>true</active>
        <description>The entered lottery numbers do not match. Please ensure the &quot;Lottery Number&quot; and &quot;Lottery Number (re-enter)&quot; fields are identical.</description>
        <errorConditionFormula>AND(
(Lottery_Number__c != Lottery_Number_re_enter__c), 
NOT(ISBLANK (Lottery_Number_re_enter__c))
)</errorConditionFormula>
        <errorDisplayField>Application_Status__c</errorDisplayField>
        <errorMessage>The entered lottery numbers do not match. Please ensure the &quot;Lottery Number&quot; and &quot;Lottery Number (re-enter)&quot; fields are identical and the numbers entered are accurate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Lottery_Number_Before_Submit</fullName>
        <active>true</active>
        <description>Lottery numbers cannot be entered before the Housing Application is submitted.</description>
        <errorConditionFormula>ISPICKVAL ( Application_Status__c, &apos;Not Yet Submitted&apos; ) &amp;&amp;
!ISBLANK ( Lottery_Number__c )</errorConditionFormula>
        <errorMessage>Lottery numbers cannot be entered before the Housing Application is submitted.</errorMessage>
    </validationRules>
