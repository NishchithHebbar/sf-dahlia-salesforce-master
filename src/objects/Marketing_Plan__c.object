    <validationRules>
        <fullName>First_Supe_Office_Date</fullName>
        <active>true</active>
        <description>The Date of Supervisor Annoucement must be in the 3 weeks after launch date.</description>
        <errorConditionFormula>(Proposed_Marketing_Launch_Date__c   &gt;=  Date_1_Supe_Office_Announcement__c)  ||   (Date_1_Supe_Office_Announcement__c  &gt;=  Proposed_Marketing_Launch_Date__c + 21)</errorConditionFormula>
        <errorDisplayField>Date_1_Supe_Office_Announcement__c</errorDisplayField>
        <errorMessage>The First Announcement in the Board of Supervisors Office must be within 21 days of the Marketing Launch Date and After the Marketing Launch Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_std_users_can_edit_approved_mktg</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISPICKVAL($Profile.UserType, &apos;Standard&apos;)) &amp;&amp; ISPICKVAL(Status__c, &apos;Approved&apos;)</errorConditionFormula>
        <errorMessage>Only MOHCD staff can edit approved marketing plans</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Second_Supe_Office_Date</fullName>
        <active>true</active>
        <description>The Date of Supervisor Annoucement must be in the 3 weeks after launch date.</description>
        <errorConditionFormula>(Proposed_Marketing_Launch_Date__c   &gt;=  Date_2_Supe_Office_Announcement__c)  ||   (Date_2_Supe_Office_Announcement__c  &gt;=  Proposed_Marketing_Launch_Date__c + 21)  || (Date_1_Supe_Office_Announcement__c = Date_2_Supe_Office_Announcement__c)</errorConditionFormula>
        <errorDisplayField>Date_2_Supe_Office_Announcement__c</errorDisplayField>
        <errorMessage>The Second Announcement in the Board of Supervisors Office must be within 21 days of the Marketing Launch Date and After the Marketing Launch Date. It also must not be the same as the First Announcement Date</errorMessage>
    </validationRules>
